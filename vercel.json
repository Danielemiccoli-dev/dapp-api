{
  "version": 2,
  "builds": [
    {
      "src": "backend/redis.js",
      "use": "@vercel/node"
    },
    {
      "src": "backend/server.js",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "backend/server.js" }
  ]
}